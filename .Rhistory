create_fe_row <- function(coefs, n_models = 2){
new_rows <- tibble(
term = "Election FE",
models = rep("Yes", times = n_models),
name = paste0("Model ", 1:n_models)
) %>% tidyr::pivot_wider(., id_cols = "term",
names_from = name, values_from = models)
attr(new_rows, "position") <- (length(coefs) * 2) + 1
new_rows
}
